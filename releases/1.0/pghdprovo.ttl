###  https://w3id.org/pghdprovo#PGHD
:PGHD rdf:type owl:Class ;
      rdfs:subClassOf <http://www.w3.org/ns/prov#Entity> ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://www.w3.org/ns/prov#wasDerivedFrom> ;
                        owl:someValuesFrom <https://saref.etsi.org/saref4wear/Wearable>
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasContextualInfo ;
                        owl:someValuesFrom :ContextualInfo
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :wasCollectedBy ;
                        owl:someValuesFrom <http://www.w3.org/ns/prov#Person>
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://www.w3.org/ns/prov#wasAttributedTo> ;
                        owl:allValuesFrom :Patient
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://www.w3.org/ns/prov#wasGeneratedBy> ;
                        owl:allValuesFrom :PGHDRequest
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :dataSource ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onDataRange xsd:string
                      ] ;
      rdfs:comment "Patient Generated Health Data (PGHD) a term referring to data that patients (or their caregivers) collect from home or any location outside the clinical environment." ;
      rdfs:label "PGHD" .


###  https://w3id.org/pghdprovo#PGHDRequest
:PGHDRequest rdf:type owl:Class ;
